{
"version": 2,
"builds": [
    {
    "src": "package.json",
    "use": "@vercel/static-build"
    }
],
"routes": [
    {
    "src": "/assets/(.*)",
    "dest": "/assets/$1"
    },
    {
    "src": "/src/(.*)",
    "dest": "/src/$1"
    },
    {
    "src": "/(.*\\.(js|css|png|jpg|svg|json|txt|map))",
    "dest": "/$1"
    },
    {
    "src": "/(.*)",
    "dest": "/index.html"
    }
]
}
